{
  "manifest_version": 2,
  "name": "PlaybackRateControll",
  "version": "1.0",
  "content_scripts":[
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["contentscript.js"]
    }
  ],
  "background": {
    "scripts": ["backgroundscript.js"]
  },
  "browser_action":{
    "default_popup":"popup.html",
    "default_title": "popup is here"
  }
}
